{{ form_start(form) }}

    {{ form_errors(form) }}

    {{ include('gif/form/date-widget.html.twig') }}

    <div class="form-group">
        {{ form_widget(form.timezone, { 'attr': {
            'autocomplete': 'off',
            'class': 'form-control'
        } }) }}
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Go</button>
    </div>

    {% if embed_code %}
        <div class="form-group">
            <textarea class="form-control" rows="10" cols="50">{{ embed_code }}</textarea>
        </div>
        <div class="form-group">
            {{ embed_code | raw }}
        </div>
    {% endif %}

{{ form_end(form) }}
